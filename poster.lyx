\[
P(K|\overline{R})=\frac{0,07}{0,83}=0,08
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
P(\overline{K}|R)=\frac{0,03}{0,17}=0,18
\]

\end_inset


\end_layout

\begin_layout Plain Layout
